QPushButton {
    background-color: rgba(255, 255, 255, 0.0);
    border-style: dashed;
    border-width: 2px;
    border-radius: 3px;
    border-color: #bfbfbf;
    font: bold 14px;
    min-width: 10em;
    padding: 6px;
    font-size: 14px;
    color: rgba(38, 38, 38, 0.8);
}

QPushButton:pressed {
    border-color: #40a9ff;
    color: #1890ff;
}
